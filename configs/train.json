{
    "model": {
        "name": "fine-tuned",
        "backbone": "resnet50",
        "freeze": true,
        "weights_path": "models/pretrained.pt"
    },
    "data": {
        "path": "data/",
        "input_shape": [
            96,
            96
        ],
        "num_classes": 10
    },
    "train": {
        "epochs": 90,
        "batch_size": 128,
        "lr": 3E-4,
        "momentum": 0.9,
        "weight_decay": 0.0,
        "log_interval": 100
    },
    "device": "cuda"
}