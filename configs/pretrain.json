{
  "model": {
    "backbone": "resnet50",
    "latent_dim": 2048,
    "proj_hidden_dim": 2048,
    "pred_hidden_dim": 512,
    "pretrained": false
  },
  "data": {
    "path": "data/",
    "input_shape": [96, 96]
  },
  "train": {
    "epochs": 100,
    "batch_size": 128,
    "lr": 0.05,
    "momentum": 0.9,
    "weight_decay": 0.0001,
    "log_interval": 100
  },
  "device": "cuda",
}